<h1>Experiments Homepage</h1>
<div>
<% if @users %>
<ul>
  <% @users.each do |user| %>
  <li>
    <a href = ""><%=user.first_name + " " + user.last_name + "| " user.email %></a>
  </li>
  <% end %>
</ul>
<% end>
</div>
<div>
<% if @experiments %>
  <ul>
  <% @experiments.each do |experiment| %>
    <li>
      <a href=""><%= experiment.title %></a>
    </li>
  <% end%>
  </ul>
<%end%>
</div>
